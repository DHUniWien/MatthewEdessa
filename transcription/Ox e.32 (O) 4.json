          "_tpen_creator" : 1923,
          "modified" : "2018-02-27 01:29:38.0"
        }, {
          "@id" : "http://t-pen.org/TPEN/line/103893461",
          "_tpen_line_id" : "line/103893461",
          "@type" : "oa:Annotation",
          "motivation" : "oad:transcribing",
          "resource" : {
            "@type" : "cnt:ContentAsText",
            "cnt:chars" : "ց խլիճասլան, լուեալ զգալ զաւրաց փռ<abbr>գ</abbr>ց "
          },
          "on" : "http://t-pen.org/TPEN/canvas/13234794#xywh=19,170,619,37",
          "_tpen_note" : "",
          "_tpen_creator" : 1923,
          "modified" : "2018-02-27 01:31:02.0"
        }, {
          "@id" : "http://t-pen.org/TPEN/line/103893462",
          "_tpen_line_id" : "line/103893462",
          "@type" : "oa:Annotation",
          "motivation" : "oad:transcribing",
          "resource" : {
            "@type" : "cnt:ContentAsText",
            "cnt:chars" : "գրեաց ի նիկիսարն յամիրայն դանիշմանն և այլ "
          },
          "on" : "http://t-pen.org/TPEN/canvas/13234794#xywh=19,207,619,37",
          "_tpen_note" : "",
          "_tpen_creator" : 1923,
          "modified" : "2018-02-27 01:32:04.0"
        }, {
          "@id" : "http://t-pen.org/TPEN/line/103893463",
          "_tpen_line_id" : "line/103893463",
          "@type" : "oa:Annotation",
          "motivation" : "oad:transcribing",
          "resource" : {
            "@type" : "cnt:ContentAsText",
            "cnt:chars" : "ամիրայսն և ահագին բ<abbr>զ</abbr>մու<abbr>բ</abbr> գայր ի <abbr>վ</abbr>ր զաւ<abbr>րց</abbr>ն "
          },
          "on" : "http://t-pen.org/TPEN/canvas/13234794#xywh=19,244,619,38",
          "_tpen_note" : "",
          "_tpen_creator" : 1923,
          "modified" : "2018-02-27 01:33:03.0"
        }, {
          "@id" : "http://t-pen.org/TPEN/line/103893464",
          "_tpen_line_id" : "line/103893464",
          "@type" : "oa:Annotation",
          "motivation" : "oad:transcribing",
          "resource" : {
            "@type" : "cnt:ContentAsText",
            "cnt:chars" : "փռանգաց․ և հանդիպեցան մի<add place=\"above\">մ</add>եանց ի դաշտն "
          },
          "on" : "http://t-pen.org/TPEN/canvas/13234794#xywh=19,282,619,34",
          "_tpen_note" : "",
          "_tpen_creator" : 1923,
          "modified" : "2018-02-27 01:34:38.0"
        }, {
          "@id" : "http://t-pen.org/TPEN/line/103893465",
          "_tpen_line_id" : "line/103893465",
          "@type" : "oa:Annotation",
          "motivation" : "oad:transcribing",
          "resource" : {
            "@type" : "cnt:ContentAsText",
            "cnt:chars" : "որ կոչի աւլաւսի, և արարին պատերազմ ահա"
          },
          "on" : "http://t-pen.org/TPEN/canvas/13234794#xywh=19,316,619,45",
          "_tpen_note" : "",
          "_tpen_creator" : 1923,
          "modified" : "2018-02-27 01:35:06.0"
        }, {
          "@id" : "http://t-pen.org/TPEN/line/103893466",
          "_tpen_line_id" : "line/103893466",
          "@type" : "oa:Annotation",
          "motivation" : "oad:transcribing",
          "resource" : {
            "@type" : "cnt:ContentAsText",
            "cnt:chars" : "գին զմեծ ժամ աւուրն, և լցաւ դաշտն առհա"
          },
