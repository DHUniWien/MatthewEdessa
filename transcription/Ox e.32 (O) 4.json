          "@type" : "oa:Annotation",
          "motivation" : "oad:transcribing",
          "resource" : {
            "@type" : "cnt:ContentAsText",
            "cnt:chars" : "գալ ի վ<abbr>ր</abbr> ն<abbr>ոց</abbr> և առնել զն<abbr>ոս</abbr> պատուհասակոծ "
          },
          "on" : "http://t-pen.org/TPEN/canvas/13234794#xywh=19,98,619,38",
