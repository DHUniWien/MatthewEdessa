          "@type" : "oa:Annotation",
          "motivation" : "oad:transcribing",
          "resource" : {
            "@type" : "cnt:ContentAsText",
            "cnt:chars" : "իրացն․ և գումարեալ դէգէ՛րէին որ<abbr>պ</abbr> զանասո"
