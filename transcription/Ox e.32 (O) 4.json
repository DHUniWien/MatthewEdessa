          "@type" : "oa:Annotation",
          "motivation" : "oad:transcribing",
          "resource" : {
            "@type" : "cnt:ContentAsText",
            "cnt:chars" : "նկաց ի աւտար աշխարհին․ և ոչ գտանէին ելս "
