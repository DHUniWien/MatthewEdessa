          "@type" : "oa:Annotation",
          "motivation" : "oad:transcribing",
          "resource" : {
            "@type" : "cnt:ContentAsText",
            "cnt:chars" : "վասն մեղաց։ Յայնժամ սուլտանն արևմտի"
          },
          "on" : "http://t-pen.org/TPEN/canvas/13234794#xywh=19,136,619,34",
