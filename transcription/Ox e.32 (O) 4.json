          "@type" : "oa:Annotation",
          "motivation" : "oad:transcribing",
          "resource" : {
            "@type" : "cnt:ContentAsText",
            "cnt:chars" : "և տեսեալ պէտևինն զբեկումն զաւրաց իւրոց "
